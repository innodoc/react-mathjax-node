{
  "name": "@innodoc/react-mathjax-node",
  "version": "0.1.0",
  "description": "React hook for integrating MathJax v3",
  "main": "cjs/",
  "module": "esm/",
  "repository": {
    "type": "git",
    "url": "https://gitlab.tu-berlin.de/innodoc/react-mathjax-node"
  },
  "bugs": {
    "url": "https://github.com/innodoc/react-mathjax-node/issues"
  },
  "author": "Mirko Dietrich <dietrich@math.tu-berlin.de>",
  "license": "MIT",
  "private": false,
  "files": [
    "/cjs",
    "/esm",
    "/src"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "mathjax"
  ],
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "build:cjs": "babel --delete-dir-on-start --source-maps --out-dir cjs --env-name cjs src",
    "build:esm": "babel --delete-dir-on-start --source-maps --out-dir esm src",
    "build:examples": "webpack --config webpack.config.js",
    "build": "yarn build:cjs && yarn build:esm",
    "coverage": "jest --coverage --silent",
    "format": "prettier --write \"*.js\" \"{src,test}/**/*.js\"",
    "lint": "eslint .",
    "test": "jest --verbose"
  },
  "dependencies": {
    "mathjax-full": "^3.0.5",
    "prop-types": "^15.7.2",
    "use-debounce": "^3.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  }
}
